@model WebApplication1.Models.CourseStudent

@{
    ViewData["Title"] = "Delete CourseStudent";
}

<h1>Delete CourseStudent</h1>
<hr />

<h3>Are you sure you want to delete this?</h3>
<dl class="row">
    <dt class="col-sm-2">Degree</dt>
    <dd class="col-sm-10">@Html.DisplayFor(model => model.Degree)</dd>

    <dt class="col-sm-2">Course</dt>
    <dd class="col-sm-10">@Model.Course?.Name</dd>

    <dt class="col-sm-2">Student</dt>
    <dd class="col-sm-10">@Model.Student?.Name</dd>
</dl>

<form asp-action="Delete">
    <input type="hidden" asp-for="Id" />
    <input type="submit" value="Delete" class="btn btn-danger" />
    <a asp-action="Index" class="btn btn-secondary">Back to List</a>
</form>